{
  "name": "ollie-notes",
  "version": "1.0.0",
  "description": "A nice way to take notes",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "dc:build": "docker-compose up -d --build",
    "dc:build:web": "docker-compose up -d --build web",
    "destroy": " rm -rf node_modules && docker-compose down",
    "eject": "react-scripts eject",
    "in:db": "docker-compose exec db psql ollie root",
    "in:web": "docker-compose exec web bash",
    "kickoff": "npm install && npm run dc:build && npm run webpack:watch",
    "log:web": "docker-compose logs -f web",
    "migrate:latest": "docker-compose exec web npx knex migrate:latest",
    "migrate:make": "npx knex migrate:make",
    "seed:latest": "docker-compose exec web npx knex migrate:latest",
    "start": "docker-compose up -d && npm webpack:watch",
    "stop": "docker-compose stop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack:build": "docker-compose exec web npx react-scripts build",
    "webpack:watch": "docker-compose exec web npx react-scripts start"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "boom": "7.x.x",
    "confidence": "4.x.x",
    "dotenv": "6.x.x",
    "glue": "5.x.x",
    "haute-couture": "3.x.x",
    "inert": "^5.1.2",
    "joi": "14.x.x",
    "knex": "^0.16.3",
    "nodemon": "^1.18.10",
    "objection": "^1.6.4",
    "pg": "^7.8.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8",
    "toys": "2.x.x"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-hapi": "12.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "hpal-debug": "1.x.x",
    "hapi": "^18.1.0",
    "bossy": "^4.0.3"
  }
}
